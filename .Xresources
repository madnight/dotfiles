Xft.dpi: 120

*TkTheme: clearlooks
Xcursor.theme: DMZ-White
URxvt.scrollBar: false
URxvt.intensityStyles: false
xterm*foreground: #c5c8c6
xterm*background: #181715
xterm*internalBorder: 0
XTerm*visualBell: False

! ##############
! xterm settings
! ##############
xterm*font:                 -*-tamzenforpowerline-medium-*-*-*-20-*-*-*-*-*-*-*
xterm*boldFont:             -*-tamzenforpowerline-bold-*-*-*-20-*-*-*-*-*-*-
URxvt*font:                 -*-terminus-medium-*-*-*-22-*-*-*-*-*-*-*
URxvt*boldFont:             -*-terminus-bold-*-*-*-22-*-*-*-*-*-*-*
URxvt.letterSpace: 3
xterm*metaSendsEscape: true
! xterm*font: -*-terminus-*-*-*-*-20-*-*-*-*-*-*-*
xterm*saveLines:            1000
xterm*geometry:             80x24-500+400
xterm*allowBoldFonts:       true
xterm*utf8:                 true
xterm*termName:             xterm-256color
xterm*VT100*translations:   #override\nShift Ctrl <KeyPress> v:insert-selection(CLIPBOARD)\nShift Ctrl <KeyPress> c:copy-selection(CLIPBOARD)
xterm.scaleHeight: 0.9

urxvt.font:                 -*-tamzenforpowerline-medium-*-*-*-20-*-*-*-*-*-*-*
urxvt.boldFont:             -*-tamzenforpowerline-bold-*-*-*-20-*-*-*-*-*-*-*
urxvt.perl-ext-common:      default -searchable-scrollback,matcher,keyboard-select,clipboard



